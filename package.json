  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(j|t)s": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "files": [
    "app/**/*.{(js|ts)x?}"
  ],
  "engines": {
    "node": ">=12.12"
  },
  "engineStrict": true
