{
  "name": "skywalk",
  "version": "1.0.1",
  "description": "DX oriented react app",
  "main": "index.js",
  "scripts": {
    "start": "cross-env webpack-dev-server --mode development --socket ./sockets/webpack.sock --env.SKYWALK_API_HOST=$SKYWALK_API_HOST --env.MIXPANEL_TOKEN=$MIXPANEL_TOKEN --env.PORT=$PORT && npm run check-socket",
    "check-socket": "node check-socket.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --mode production --env.SKYWALK_API_HOST=$SKYWALK_API_HOST --env.MIXPANEL_TOKEN=$MIXPANEL_TOKEN",
    "commit": "npx git-cz",
    "cu": "npm-check -u",
    "lint": "npx pretty-quick && npx eslint './app/**/*.{ts,tsx}' --fix",
    "test": "rimraf coverage/* && jest --detectOpenHandles --maxWorkers=1",
    "test:watch": "jest -c ./config/test/jest.json --verbose --watchAll -i",
    "tslint-check": "tslint-config-prettier-check ./tslint.yaml"
  },
  "keywords": [],
  "author": "https://github.com/g-six",
  "license": "ISC",
