{
  "name": "skywalk",
  "version": "1.12.12",
  "description": "DX oriented react app",
  "main": "index.js",
  "scripts": {
    "start": "cross-env webpack-dev-server --mode development --socket ./sockets/webpack.sock --env.SKYWALK_API_HOST=$SKYWALK_API_HOST --env.PORT=$PORT && npm run check-socket",
    "check-socket": "node check-socket.js",
    "build": "rimraf dist/* && cross-env NODE_ENV=production webpack --mode production --env.SKYWALK_API_HOST=$SKYWALK_API_HOST",
    "commit": "npx git-cz",
    "cu": "npm-check -u",
    "lint": "npx pretty-quick && npx eslint './app/**/*.{ts,tsx}' --fix",
    "test": "rimraf coverage/* && jest --detectOpenHandles --maxWorkers=1",
    "test:watch": "jest -c ./config/test/jest.json --verbose --watchAll -i",
    "tslint-check": "tslint-config-prettier-check ./tslint.yaml"
  },
  "keywords": [],
  "author": "https://github.com/g-six",
  "license": "ISC",
  "dependencies": {
    "connected-react-router": "^6.5.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-syntax-highlighter": "^11.0.2",
    "redux": "^4.0.4",
    "styled-components": "^5.2.3",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@testing-library/dom": "^7.30.4",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-syntax-highlighter": "^11.0.1",
    "@types/redux-mock-store": "^1.0.1",
    "@types/styled-components": "^5.1.9",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.21.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-redux-saga": "^1.1.0",
    "bulma": "^0.7.5",
    "commitlint": "^8.2.0",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^7.25.0",
