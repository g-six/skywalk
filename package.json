    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "raf": "^3.4.1",
    "raw-loader": "^3.1.0",
    "react-hot-loader": "^4.12.15",
    "redux-mock-store": "^1.5.3",
    "reselect": "^4.0.0",
